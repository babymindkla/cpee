#include <stdio.h>

void BMI();

int main(){
  BMI();
  return 0;
}

void BMI(){
  float h,w;
  printf("Enter your height (m): ");
  scanf("%f",&h);
  printf("Enter your weight (kg): ");
  scanf("%f",&w);
  float bmi = w/(h*h);
  printf("Your BMI = %.2f and you are ",bmi);
  if (bmi >= 30) printf("obese.");
  else if (bmi >=25 ) printf("overweight.");
  else if (bmi >= 18.5) printf("normal. ");
  else printf("underweight");
}
